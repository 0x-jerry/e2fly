    "test:main": "vitest run -r src/main --passWithNoTests",
    "test:e2e": "vitest run",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "gen:icon": "tsx scripts/make-icon.ts",
    "release": "x-release"
  },
  "dependencies": {
