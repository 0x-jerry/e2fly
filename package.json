    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.1",
    "electron": "^18.0.4",
    "electron-builder": "^23.0.3",
    "esbuild": "^0.14.36",
    "picocolors": "^1.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
