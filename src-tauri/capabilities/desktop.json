{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "desktop-capability",
  "platforms": [
    "macOS",
    "windows",
    "linux"
  ],
  "webviews": [
    "main"
  ],
  "local": true,
  "permissions": [
    "core:default",
    "updater:default",
    "autostart:default",
    "clipboard-manager:default",
    "shell:default",
    "core:event:allow-listen",
    "core:window:allow-hide",
    "window-state:default"
  ]
}